{
  "name": "museeks",
  "productName": "Museeks",
  "version": "0.11.4",
  "description": "A simple, clean and cross-platform music player, written with Node.js, Electron and React.js.",
  "main": "./dist/main/bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/martpie/museeks"
  },
  "author": "Pierre de la Martini√®re <pierre.de.la.martiniere@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/martpie/museeks/issues"
  },
  "scripts": {
    "build": "webpack --config webpack.prod.js --env.production --colors",
    "dev": "webpack --config webpack.dev.js --colors --watch",
    "museeks": "electron .",
    "museeks:debug": "electron . --enable-logging --devtools",
    "modules:rebuild": "electron-rebuild -f",
    "test:lint": "eslint . --ext .tsx,.ts,.js",
    "test:lint:fix": "eslint . --ext .tsx,.ts,.js --fix",
    "test:formatting": "prettier --check \"./**/*.{ts,tsx,js}\"",
    "test:formatting:fix": "prettier --write \"./**/*.{ts,tsx,js}\"",
    "test:css": "stylelint \"src/**/*.css\"",
    "package:lmw": "electron-builder -lmw --ia32 --x64 && npm run package:checksums",
    "package:l": "electron-builder -l --ia32 --x64",
    "package:w": "electron-builder -w --ia32 --x64",
    "package:m": "electron-builder -m",
    "package:checksums": "bash scripts/checksum.sh"
  },
  "dependencies": {
    "bluebird": "3.7.2",
    "chardet": "0.8.0",
    "classnames": "2.2.6",
    "font-awesome": "4.7.0",
    "globby": "11.0.0",
    "history": "4.10.1",
    "iconv-lite": "0.5.1",
    "level-js": "4.0.1",
    "linvodb3": "3.26.0",
    "lodash-es": "4.17.15",
    "m3ujs": "0.2.1",
    "mime-types": "2.1.26",
    "mpris-service": "2.1.0",
    "music-metadata": "6.3.4",
    "postcss-scss": "2.0.0",
    "ps-node": "0.1.6",
    "queue": "6.0.1",
    "react": "16.12.0",
    "react-custom-scrollbars": "4.2.1",
    "react-dom": "16.12.0",
    "react-fontawesome": "1.7.1",
    "react-keybinding-component": "1.0.0",
    "react-onclickout": "2.0.8",
    "react-rangeslider": "2.2.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-simple-input": "0.4.2",
    "redux": "4.0.5",
    "redux-thunk": "2.3.0",
    "semver": "7.1.3",
    "svg-inline-react": "3.2.0",
    "teeny-conf": "4.0.0"
  },
  "devDependencies": {
    "@types/bluebird": "3.5.29",
    "@types/chardet": "0.8.0",
    "@types/classnames": "2.2.9",
    "@types/history": "4.7.5",
    "@types/lodash-es": "4.17.3",
    "@types/mime-types": "2.1.0",
    "@types/react": "16.9.19",
    "@types/react-custom-scrollbars": "4.0.6",
    "@types/react-dom": "16.9.5",
    "@types/react-fontawesome": "1.6.4",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "5.1.3",
    "@types/redux-logger": "3.0.7",
    "@types/semver": "7.1.0",
    "@typescript-eslint/eslint-plugin": "2.19.2",
    "@typescript-eslint/parser": "2.19.2",
    "clean-terminal-webpack-plugin": "2.1.0",
    "css-loader": "3.4.2",
    "electron": "8.2.0",
    "electron-builder": "22.3.2",
    "electron-rebuild": "1.10.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "install": "0.13.0",
    "mini-css-extract-plugin": "0.9.0",
    "node-loader": "0.6.0",
    "normalize.css": "8.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.1",
    "prettier": "1.19.1",
    "prettier-eslint": "9.0.1",
    "redux-logger": "3.0.6",
    "source-map-loader": "0.2.4",
    "stylelint": "13.1.0",
    "stylelint-config-css-modules": "2.2.0",
    "stylelint-config-standard": "20.0.0",
    "svg-inline-loader": "0.8.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-loader": "6.2.1",
    "typescript": "3.7.5",
    "url-loader": "2.2.0",
    "webpack": "4.41.6",
    "webpack-cli": "3.3.11",
    "webpack-merge": "4.2.2",
    "webpackbar": "4.0.0"
  }
}
